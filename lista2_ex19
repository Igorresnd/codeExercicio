#include <stdio.h>
#include <stdlib.h>
  class Main {
  public static void main(String[] args)  {
  
int n;
	
	printf ("Escolha um numero ate 1000 \n");
	scanf ("%i", &n);
	
	int c = n/100;
	int d = ((n-(c*100))/10);
	int u = (n-(c*100 + d*10));
	
	if (n<=1000){
	printf ("%i centenas \n", c);
	printf ("%i dezenas \n", d);
	printf ("%i unidades \n", u); }
	
	else printf ("Valor invalido");
	
	return 0;
}}
