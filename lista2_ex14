#include <stdio.h>
#include <stdlib.h>
  class Main {
  public static void main(String[] args)  {

float n1,n2,md;
    
    printf("Digite sua primeira nota parcial do semestre: ");
    scanf("%f",&n1);
    printf("\nDigite sua segunda nota parcial do semestre: ");
    scanf("%f",&n2);
    
    md=(n1+n2)/2;
    
    printf("\nSua média foi de: %.2f",md);
    
    if(md==0||md<4){
    printf("\nREPROVADO!\nConceito E.");
    }else if(md>=4||md<6){
        printf("\nREPROVADO!\nConceito D.");
    }else if(md>=6||md<7,5){
        printf("\nAPROVADO!\nConceito C");
    }else if(md>=7,5||md<9){
        printf("\nAPROVADO!\nConceito B.");
    }else if(md>=9||md<=10){
        printf("\nAPROVADO!\nConceito A.");
    }else{
        printf("\nMédia incorreta!");
    }
}
}}
